{
  "expo": {
    "name": "Torneo MicroFutbol",
    "slug": "torneo-microfutbol",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.felipesanchez.torneomicrofutbol"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "eas": {
      "projectId": "7254d3ee-7cdf-4283-879a-1ea609114183"
    },
    "owner": "felipesanchez-dev",
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "0ce30c8e-d94f-4fa5-b85e-45f744fea9fe"
      }
    }
  }
}
